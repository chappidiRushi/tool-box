{
  "name": "tool-box",
  "displayName": "Tool Box",
  "description": "extension for useful tools",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:tool-box.toolBox"
  ],
  "main": "./extension.js",
  "enabledApiProposals": ["contribEditorContentMenu"],
  "contributes": {
    "commands": [
      {
        "command": "tool-box.toolBox",
        "title": "Hello World"
      }
    ],
    "menus": {
      "editor/content": [
        {
          "command": "too-box.toolBox",
          "group": "toolBox"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}