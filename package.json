{
  "name": "tool-box",
  "displayName": "Tool Box",
  "description": "extension for useful tools",
  "repository": {"url": "https://github.com/chappidiRushi/tool-box"},
  "publisher": "Rushi",
  "icon": "images/tool-box-logo.png",
  "version": "0.0.5",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./extension.js",
  "contributes": {
    "commands": [
      {
        "command": "tool-box.int2func",
        "title": "Convert to InterfaceFunction"
      },
      {
        "command": "tool-box.allInt2func",
        "title": "Convert all interfaces to functions"
      },
      {
        "command": "tool-box.restartApp",
        "title": "restart application"
      },
      {
        "command": "tool-box.openTerminal",
        "title": "Open external terminal"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js",
    "build": "vsce package"
  },
  "devDependencies": {
    "@types/vscode": "^1.75.0",
    "@types/glob": "^8.0.1",
    "@types/mocha": "^10.0.1",
    "@types/node": "16.x",
    "eslint": "^8.33.0",
    "glob": "^8.1.0",
    "mocha": "^10.1.0",
    "typescript": "^4.9.4",
    "@vscode/test-electron": "^2.2.2"
  }
}